{
  "name": "my-qwik-docs-starter",
  "description": "Basic start point to build a docs site with Qwik",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "server": "json-server --watch db.json",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/core": "^0.4.0",
    "@builder.io/qwik": "0.23.0",
    "@builder.io/qwik-auth": "0.0.5",
    "@builder.io/qwik-city": "0.6.5",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/material": "5.11.9",
    "@mui/x-data-grid": "5.17.24",
    "@types/eslint": "8.21.2",
    "@types/node": "^18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.36.0",
    "eslint-plugin-qwik": "0.23.0",
    "json-server": "^0.17.3",
    "postcss": "^8.4.16",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "5.0.2",
    "undici": "5.21.0",
    "vite": "4.2.0",
    "vite-tsconfig-paths": "3.5.0"
  },
  "dependencies": {
    "@phosphor-icons/web": "^2.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-popover": "^1.0.5",
    "@types/": "phosphor-icons/web",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "firebase": "^9.19.1"
  }
}
